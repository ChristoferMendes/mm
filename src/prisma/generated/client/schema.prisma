generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
  binaryTargets = ["native"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model DefaultConfig {
  id    Int     @id @default(autoincrement())
  index Boolean
}
